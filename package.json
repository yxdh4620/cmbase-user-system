{
  "name": "cmbase-user-system",
  "description": "A demo app written in coffeescript, run on nodejs illustrating the use of passport in express, jade and mongoose environment",
  "version": "0.1.0",
  "private": false,
  "author": [
    {
      "name": "Mustafa Dikici",
      "email": "musdikici@gmail.com"
    },
